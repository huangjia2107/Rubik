<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sysRuntime="clr-namespace:System;assembly=System.Runtime"
                    xmlns:themes="clr-namespace:Rubik.Theme">

    <sysRuntime:Boolean x:Key="{x:Static themes:ResourceKeys.TrueValueKey}">True</sysRuntime:Boolean>
    <sysRuntime:Boolean x:Key="{x:Static themes:ResourceKeys.FalseValueKey}">False</sysRuntime:Boolean>

</ResourceDictionary>