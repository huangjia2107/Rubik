<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:rubik="https://github.com/huangjia2107/Rubik/"
                    xmlns:themes="clr-namespace:Rubik.Theme.VLC">

    <ResourceDictionary.MergedDictionaries>

        <ResourceDictionary Source="{x:Static rubik:ResourceKeys.RubikThemeSource}"/>
        <ResourceDictionary Source="{x:Static themes:VlcResourceKeys.VlcMediaPlayerStyleSource}"/>
        
    </ResourceDictionary.MergedDictionaries>
</ResourceDictionary>
